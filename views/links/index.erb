<h1> Links </h1>

<ul id="links">
  <% @links.each do |link| %>
    <li>
      Title: <%= link.title %>
      URL:   <%= link.url   %>
    </li>
  <% end %>
</ul>


<form action='/links/new'>
  <input type='submit' value='Create new link'>
</form>
<form action='/users/new'>
  <input type='submit' value='Sign up'>
</form>